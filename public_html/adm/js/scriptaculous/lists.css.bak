body,div,input,td {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
}

td {
	vertical-align: top;
}

ul.sortableList {
	list-style-type: none;
	padding: 0px;
	margin: 0px;
	width: 300px;
	font-family: Arial, sans-serif;
}
ul.sortableList li {
	cursor: move;
	padding: 2px 2px;
	margin: 2px 0px;
	border: 1px solid #000000;
	background-color: #daeda3;
}

div#divContainer {
	border: 1px solid #000000;
	width: 400px;
}

div#divContainer div {
	border: 1px solid #000000;
	margin: 5px;
	padding: 2px;
	cursor: move;
}

div#imageContainer {
	border: 1px solid #000000;
}

div#imageContainer img {
	/*cursor: move;*/
	float: left;
	/*display: block;*/
	margin: 5px;
	border: 1px solid #000000;
}

div#imageFloatContainer {
	border: 0px solid #000000;
	cursor: move
	text-align:center;
}

div#imageFloatContainer img {
	float: left;
	margin: 5px;
	border: 1px solid #000000;
}